{
	"manifest_version": 2,
	"name": "LinkLight",
	"version": "1.0",
	"description": "Highlights all links on a page, even hidden ones!",
	"icons": {
		"128": "icon128.png",
		"48": "icon48.png",
		"16": "icon16.png"
	},
	"background": {
		"scripts": ["eventPage.js"],
		"persistent": false
	},
	"permissions": [
		"tabs",
		"contextMenus",
		"http://*/",
        	"https://*/"
	]

}